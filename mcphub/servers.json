{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "20"
      }
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${ALLOWED_DIRECTORY}"
      ],
      "command": "npx"
    },
    "postgres": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${POSTGRESQL_URL}"
      ],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    }
  },
  "nativeMCPServers": []
}
